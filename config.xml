<?xml version='1.0' encoding='utf-8'?>
<widget id="de.gdiservice.kvmobile" version="1.9.5" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>kvwmobile</name>
    <description>
      Anwendung zur Erfassung von Geodaten und zum synchronisieren mit dem Web-GIS kvwmap.
    </description>
    <author email="peter.korduan@gdi-service.de" href="http://kvwmap.de/index.php/kvmobile">
        GDI-Service Dr. Peter Korduan
    </author>
    <content src="index.html" />
    <access origin="https://gdi-service.de/kvwmap_pet_dev/*" />
    <access origin="https://geoportal.lkros.de/kvwmap_dev/*" />
    <access origin="https://geoportal.lkros.de/kvwmap_test/*" />
    <access origin="https://geoportal.lkros.de/kvwmap_lro/*" />
    <access origin="https://geoportal-vg.de/kvwmap/*" />
    <access origin="https://geoportal-vg.de/kvwmap_test/*" />
    <access origin="https://geoport.lk-vr.de/kvwmap/*" />
    <access origin="https://geoport-lk-mse.de/kvwmap/*" />
    <access origin="https://geoport-lk-mse.de/supergis/*" />
    <access origin="https://geoportal.lkee.de/*" />
    <access origin="https://www.orka-mv.de/*" />
    <access origin="https://isk.geobasis-bb.de/mapproxy/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="file:*" />
    <allow-navigation href="*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    </platform>
    <plugin name="@mauron85/cordova-plugin-background-geolocation"/>
    <plugin name="cordova-plugin-camera" spec="^6.0.0" />
    <plugin name="cordova-plugin-compat" spec="^1.2.0" />
    <plugin name="cordova-plugin-device" spec="^2.0.3" />
    <plugin name="cordova-plugin-geolocation" spec="^4.1.0" />
    <plugin name="cordova-plugin-network-information" spec="^3.0.0" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.7.1" />
    <plugin name="cordova-plugin-dialogs" spec="^2.0.2" />
    <plugin name="cordova-sqlite-storage" spec="^6.0.0" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.5" />
    <plugin name="com.ionic.keyboard" spec="https://github.com/Telerik-Verified-Plugins/Keyboard" />
    <plugin name="cordova-plugin-file-opener2" spec="^3.0.5" />
    <icon src="res/icon/gdi-service_ball.png" platform="android" width="57" height="57" density="mdpi" />
</widget>
